{
    "name": "faczone-backend",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "directories": {
        "test": "test"
    },
    "engines": {
        "node": "16.x"
    },
    "scripts": {
        "test": "NODE_ENV=test ./node_modules/.bin/mocha --recursive --exit --config ./test/.mocharc.js",
        "test:ci": "NODE_ENV=ci ./node_modules/.bin/mocha --recursive --exit --config ./test/.mocharc.js",
        "test:watch": "NODE_ENV=test ./node_modules/.bin/supervisor -q -n exit -x ./node_modules/.bin/mocha -- --recursive --config test/.mocharc.js",
        "server": "NODE_ENV=${npm_config_env} nodemon index.js",
        "cron": "NODE_ENV=${npm_config_env} nodemon cron-index.js",
        "db:create": "NODE_ENV=${npm_config_env} sequelize-cli db:create --env=${npm_config_env}",
        "db:migrate": "NODE_ENV=${npm_config_env} sequelize-cli db:migrate --env=${npm_config_env}",
        "db:migrate:undo": "NODE_ENV=${npm_config_env} sequelize-cli db:migrate:undo --name=${npm_config_name} --env=${npm_config_env}",
        "db:seed:all": "NODE_ENV=${npm_config_env} sequelize-cli db:seed:all --env=${npm_config_env}",
        "db:seed:undo": "NODE_ENV=${npm_config_env} sequelize-cli db:seed:undo --env=${npm_config_env}",
        "seed:generate": "sequelize-cli seed:generate --name=${npm_config_name}",
        "migration:generate": "sequelize-cli migration:generate --name=${npm_config_name}",
        "db:drop": "NODE_ENV=${npm_config_env} sequelize-cli db:drop --env=${npm_config_env}",
        "coverage": "NODE_ENV=test nyc --reporter=html ./node_modules/.bin/mocha --recursive --exit --config ./test/.mocharc.js",
        "coverage:bitbucket": "NODE_ENV=test nyc --reporter=lcov ./node_modules/.bin/mocha --recursive --exit --config ./test/.mocharc.js",
        "build:env": "node build-env-index.js"
    },
    "author": "Champaran Labs <champaranlabs@gmail.com>",
    "license": "Champaran Labs",
    "dependencies": {
        "@changesets/cli": "^2.26.0",
        "app-module-path": "^2.2.0",
        "aws-sdk": "^2.1168.0",
        "axios": "^0.27.2",
        "bcrypt": "^5.1.1",
        "cls-hooked": "^4.2.2",
        "config": "^3.3.7",
        "cors": "^2.8.5",
        "cron": "^2.1.0",
        "cronitor": "^2.3.2",
        "dayjs": "^1.11.1",
        "dotenv": "^16.0.1",
        "express": "^4.18.1",
        "folktale": "^2.3.2",
        "jsonwebtoken": "^8.5.1",
        "mime": "^3.0.0",
        "minimist": "^1.2.6",
        "nodemailer": "^6.7.6",
        "nodemon": "^2.0.19",
        "npm": "^8.13.2",
        "nyc": "^15.1.0",
        "pg": "^8.7.3",
        "ramda": "^0.26.1",
        "sequelize": "^6.21.2",
        "sequelize-slugify": "^1.6.2",
        "supervisor": "^0.12.0",
        "twilio": "^3.78.0",
        "uuid": "^8.3.2",
        "uuid-validate": "^0.0.3",
        "winston": "^3.8.1",
        "winston-cloudwatch": "^6.1.1",
        "winston-daily-rotate-file": "^4.7.1",
        "ws": "^8.8.0"
    },
    "devDependencies": {
        "babel-eslint": "^9.0.0",
        "chai": "^4.2.0",
        "dependency-cruiser": "^13.1.4",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-config-google": "^0.14.0",
        "eslint-plugin-chai-friendly": "^0.4.1",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-mocha": "^5.2.0",
        "factory-girl": "^4.2.1",
        "mocha": "^5.2.0",
        "sequelize-cli": "^6.4.1",
        "sequelize-mig": "^3.1.3",
        "sinon": "^7.2.3",
        "sinon-chai": "^3.3.0"
    }
}
